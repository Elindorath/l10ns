{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es6",
        "noImplicitAny": true,
        "sourceMap": false,
		"strictNullChecks": true,
		"noImplicitReturns": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"noImplicitThis": true,
		"outDir": "Build"
    },
    "exclude": [
        "Build/"
    ]
}
